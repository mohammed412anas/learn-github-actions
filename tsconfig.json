  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",
    "types": ["jest"],                                                           
    "outDir": "dist/",  
    "rootDir":"src/",                                  
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                     
    "skipLibCheck": true                                 
